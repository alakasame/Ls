#include "ft_ls.h"

t_l		*listadd(char *str)
{
	t_l *list;

	if (!(list = (t_l *)malloc(sizeof(t_l))))
		return (0);
	list->next = NULL;
	list->back = NULL;
	list->str = ft_strdup(str);
}
